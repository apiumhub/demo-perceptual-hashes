{
    "name": "apiumhub/demo-perceptual-hashes",
    "type": "project",
    "license": "MIT",
    "description": "Reverse Image Search based on Perceptual Hashes built with PHP",
    "keywords": ["imagehash", "perceptual", "hash", "phash", "image hash", "php", "reverse image search"],
    "require": {
        "php": "^8.0",
        "jenssegers/imagehash": "^0.8.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
