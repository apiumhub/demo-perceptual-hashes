{
    "name": "apiumhub/demo-perceptual-hashes",
    "type": "project",
    "license": "MIT",
    "description": "Reverse Image Search based on Perceptual Hashes built with PHP",
    "keywords": ["imagehash", "perceptual", "hash", "phash", "image hash", "php", "reverse image search"],
    "require": {
        "php": "^8.0",
        "jenssegers/imagehash": "^0.8.0"
    },
    "require-dev": {
        "brianium/paratest": "^6.5",
        "codedungeon/phpunit-result-printer": "^0.32.0",
        "friendsofphp/php-cs-fixer": "^3.8",
        "infection/infection": "^0.26.13",
        "phpstan/phpstan": "^1.8",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UnitTests\\": "tests/Unit"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "infection/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
